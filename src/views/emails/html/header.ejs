		}

		.social-icon::hover {
			background: -webkit-linear-gradient(315deg, #2AC88F 0%, #29A9DB 49.19%, #6A2BB9 100%);background: -o-linear-gradient(315deg, #2AC88F 0%, #29A9DB 49.19%, #6A2BB9 100%);background: linear-gradient(135deg, #2AC88F 0%, #29A9DB 49.19%, #6A2BB9 100%);
		}

		/* iOS BLUE LINKS */
		a[x-apple-data-detectors] {
			color: inherit !important;
			text-decoration: none !important;
			font-size: inherit !important;
			font-family: inherit !important;
			font-weight: inherit !important;
			line-height: inherit !important;
		}

		/* ANDROID CENTER FIX */
		div[style*="margin: 16px 0;"] {
			margin: 0 !important;
		}

		/* MEDIA QUERIES */
		@media all and (max-width:639px) {
			.wrapper {
				width: 320px !important;
				padding: 0 !important;
			}

			.container {
				width: 300px !important;
				padding: 0 !important;
			}

			.mobile {
				width: 300px !important;
				display: block !important;
				padding: 0 !important;
			}

			.img {
				width: 100% !important;
				height: auto !important;
			}

			*[class="mobileOff"] {
				width: 0px !important;
				display: none !important;
			}

			*[class*="mobileOn"] {
				display: block !important;
				max-height: none !important;
			}
		}
	</style>
</head>
