{
  "name": "business-docs",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently --kill-others \"npm run chokidar\" \"npm run dev\"",
    "dev": "nodemon dotenv_config_path=.env.development --exec \"babel-node --extensions '.ts' -r node_modules/dotenv/config ./src/server.ts \"",
    "build": "babel --extensions '.ts' src --out-dir build && npm run copy-views-dir",
    "copy-views-dir": "ncp ./src/views ./build/views",
    "chokidar": "cd ../../shared/graphql && chokidar \"services/**/*.js\" -c \"npm run babel:services\"",
    "preinstall": "npm run build"
  },
  "author": "Kudoo Cloud Pty Ltd",
